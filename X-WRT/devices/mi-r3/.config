#
# Target Images
#
CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_xiaomi_mi-router-ac2100=y
# end of Target Images

# IMAGEOPT
CONFIG_IMAGEOPT=y
CONFIG_PREINITOPT=y
CONFIG_TARGET_PREINIT_IFNAME=""
CONFIG_TARGET_PREINIT_IP="192.168.31.1"
CONFIG_TARGET_PREINIT_NETMASK="255.255.255.0"
CONFIG_TARGET_PREINIT_BROADCAST="192.168.31.255"
CONFIG_INITOPT=y
CONFIG_TARGET_INIT_PATH="/usr/sbin:/usr/bin:/sbin:/bin"
CONFIG_TARGET_INIT_ENV=""
CONFIG_TARGET_INIT_CMD="/sbin/init"
CONFIG_TARGET_INIT_SUPPRESS_STDERR=y
CONFIG_VERSIONOPT=y
CONFIG_VERSION_DIST="X-WRT"
CONFIG_VERSION_NUMBER="22.03_b202204240424"
CONFIG_VERSION_CODE="Jammy"
CONFIG_VERSION_HOME_URL="https://x-wrt.com/"
CONFIG_VERSION_MANUFACTURER="X-WRT-TEAM"
CONFIG_VERSION_MANUFACTURER_URL="https://x-wrt.com/"
CONFIG_VERSION_BUG_URL="https://x-wrt.com/"
CONFIG_VERSION_SUPPORT_URL="https://x-wrt.com/"
CONFIG_VERSION_PRODUCT="X-Router"
CONFIG_VERSION_HWREV=""
CONFIG_VERSION_FILENAMES=n
CONFIG_VERSION_CODE_FILENAMES=y
CONFIG_PER_FEED_REPO=y
CONFIG_FEED_packages=y
CONFIG_FEED_luci=y
CONFIG_FEED_routing=y
CONFIG_FEED_telephony=y
# CONFIG_FEED_x is not set
# CONFIG_FEED_zhuxiaole is not set
# CONFIG_FEED_onlyme is not set

# system
CONFIG_PACKAGE_base-config-setting=y
CONFIG_LUCI_LANG_zh_Hans=y
